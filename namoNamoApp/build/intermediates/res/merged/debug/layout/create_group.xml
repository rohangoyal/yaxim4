<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
<include layout="@layout/toolbar_group"
    android:id="@+id/toolbar_group"></include>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:background="#d3d3d3"
        android:layout_below="@+id/toolbar_group">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:id="@+id/imageView"
            android:src="@drawable/backheaderimage"
            android:padding="2dp"
            android:scaleType="fitXY"
            android:background="@drawable/pic_back"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imageView2"
            android:src="@drawable/circleuser"
            android:layout_marginLeft="31dp"
            android:layout_marginStart="31dp"
            android:layout_alignTop="@+id/imageView14" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView2"
            android:background="@drawable/textview_back"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="10dp"
            android:id="@+id/linearLayout13">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/titleid"
                android:hint="Title"
                android:fontFamily="sans-serif-condensed"
                android:textSize="17dp"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/titleimg"
                        android:src="@drawable/title"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="right"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:id="@+id/ImageView92"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />

                </RelativeLayout>
            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linearLayout13"
            android:background="@drawable/textview_back"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="10dp"
            android:id="@+id/linearLayout">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/adminid"
                android:hint="Admin Name"
                android:fontFamily="sans-serif-condensed"
                android:textSize="17dp"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:id="@+id/imageView3"
                    android:src="@drawable/admin"
                    android:layout_marginLeft="20dp"
                    android:layout_gravity="right"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true" />

                    <ImageView
                        android:id="@+id/ImageView02"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />

                </RelativeLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linearLayout"
            android:background="@drawable/textview_back"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:id="@+id/linearLayout2">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/groupid"
                android:fontFamily="sans-serif-condensed"
                android:hint="Group/Org/Community"
                android:textSize="17dp"
                android:singleLine="true"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/imageView4"
                        android:src="@drawable/org"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="right"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />
                    <ImageView
                        android:id="@+id/ImageView01"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />



                </RelativeLayout>

            </LinearLayout>

        </LinearLayout>




        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linearLayout2"
            android:background="@drawable/textview_back"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:id="@+id/linearLayout3">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/aboutusid"
            android:hint="About us"
            android:fontFamily="sans-serif-condensed"
            android:textSize="17dp"
            android:textColor="#757575"
            android:padding="20dp"
            android:layout_weight=".1" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight=".4">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:id="@+id/imageView5"
                    android:src="@drawable/aboutuss"
                    android:layout_marginLeft="20dp"
                    android:layout_gravity="right"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true" />

                <ImageView
                    android:id="@+id/ImageView03"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    android:src="@drawable/divider_attach_vertical" />

            </RelativeLayout>
        </LinearLayout>
</LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/textview_back"
            android:layout_below="@+id/linearLayout3"
            android:layout_marginTop="10dp"
            android:id="@+id/linearLayout4"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/alternatephoneid"
                android:hint="Alternate Mobile No"
                android:textSize="17dp"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/imageView7"
                        android:src="@drawable/callcontact"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="right"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:id="@+id/ImageView04"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/textview_back"
            android:layout_below="@+id/linearLayout4"
            android:id="@+id/linearLayout5"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/maincateid"
                android:hint="Select Main Category Type"
                android:textSize="17dp"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/imageView6"
                        android:src="@drawable/categoryy"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="right"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:id="@+id/ImageView05"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/textview_back"
            android:layout_below="@+id/linearLayout5"
            android:id="@+id/linearLayout6"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/subcategoryid"
                android:fontFamily="sans-serif-condensed"
                android:hint="Select Sub Category Type"
                android:textSize="17dp"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/imageView8"
                        android:src="@drawable/categoryy"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="right"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:id="@+id/ImageView06"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/textview_back"
            android:layout_below="@+id/linearLayout6"
            android:id="@+id/linearLayout7"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/emailid"
                android:hint="E-Mail Id"
                android:fontFamily="sans-serif-condensed"
                android:textSize="17dp"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/imageView9"
                        android:src="@drawable/mail"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="right"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:id="@+id/ImageView07"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/textview_back"
            android:layout_below="@+id/linearLayout7"
            android:id="@+id/linearLayout8"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/websiteid"
                android:hint="Website Url"
                android:textSize="17dp"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/imageView10"
                        android:src="@drawable/website"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="right"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:id="@+id/ImageView08"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/textview_back"
            android:layout_below="@+id/linearLayout8"
            android:id="@+id/linearLayout9"
            android:visibility="gone"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/stateid"
                android:hint="State"
                android:fontFamily="sans-serif-condensed"
                android:textSize="17dp"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/imageView11"
                        android:src="@drawable/state"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="right"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:id="@+id/ImageView09"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/textview_back"
            android:layout_below="@+id/linearLayout9"
            android:id="@+id/linearLayout10"
            android:visibility="gone"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/cityid"
                android:hint="City"
                android:fontFamily="sans-serif-condensed"
                android:textSize="17dp"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/imageView12"
                        android:src="@drawable/city"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="right"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:id="@+id/ImageView10"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/textview_back"
            android:layout_below="@+id/linearLayout10"
            android:id="@+id/linearLayout11"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/addressid"
                android:hint="Address"
                android:fontFamily="sans-serif-condensed"
                android:textSize="17dp"
                android:textColor="#757575"
                android:padding="20dp"
                android:layout_weight=".1" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".4">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/imageView13"
                        android:src="@drawable/addlocation"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="right"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:id="@+id/ImageView11"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:src="@drawable/divider_attach_vertical" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linearLayout11"
            android:id="@+id/linearLayout12"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/submitid"
                android:text="Submit"
                android:background="@drawable/submit_back"
                android:textSize="15dp"
                android:fontFamily="sans-serif-condensed"
                android:textStyle="bold"
                android:textColor="#fff"
                android:padding="15dp"
                android:gravity="center"/>
        </LinearLayout>

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/edit"
            android:id="@+id/imageView14"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:layout_alignBottom="@+id/imageView"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView15"
            android:layout_alignBottom="@+id/imageView2"
            android:layout_alignRight="@+id/imageView2"
            android:layout_alignEnd="@+id/imageView2" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/camera"
            android:src="@drawable/edit"
            android:layout_alignBottom="@+id/imageView2"
            android:layout_alignRight="@+id/imageView2"
            android:layout_alignEnd="@+id/imageView2" />
    </RelativeLayout>
</LinearLayout>
    </ScrollView>
</RelativeLayout>
